{
  "private": false,
  "name": "trpc-uwebsockets",
  "description": "tRPC adapter for uWebSockets.js server",
  "version": "0.10.0-proxy-beta.10.1",
  "main": "./dist/index.js",
  "type": "commonjs",
  "types": "./types/index.d.ts",
  "scripts": {
    "start": "jest --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:publish": "yarn test && yarn build && yarn pack",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn prettier && yarn lint && yarn test && yarn build",
    "clean": "rimraf -rf ./dist && rimraf -rf ./types && rimraf -rf ./*-*.tgz",
    "format": "yarn prettier && yarn lint",
    "lint": "eslint . --ext .ts --fix",
    "lint:dry": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier:dry": "prettier --config .prettierrc 'src/**/*.ts'"
  },
  "dependencies": {
    "@trpc/server": "^10.0.0-proxy-beta.10",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@swc/core": "^1.2.215",
    "@swc/jest": "^0.2.21",
    "@trpc/client": "^10.0.0-proxy-beta.10",
    "@types/cookie": "^0.5.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/node-fetch": "^2.6.2",
    "@types/webrtc": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "abort-controller": "^3.0.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^27.1.0",
    "jest-environment-node": "^28.1.3",
    "node-fetch": "2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-swc": "^0.2.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4",
    "zod": "^3.17.9"
  },
  "files": [
    "dist",
    "src",
    "types",
    "test"
  ],
  "license": "MIT",
  "keywords": [
    "trpc",
    "uwebsockets",
    "trpc adapter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/romanzy-1612/trpc-uwebsockets.git"
  },
  "author": "Roman Volovoy"
}
